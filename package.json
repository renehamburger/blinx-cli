{
  "name": "blinx.js-crawler",
  "version": "0.0.1",
  "description": "Node.js app to analyse a website using blinx.js",
  "engines": {
    "node": ">= 12.9.0"
  },
  "scripts": {
    "start": "ts-node --transpile-only src/index.ts",
    "watch": "nodemon -r ts-node/register src/index.ts",
    "debug": "nodemon --inspect-brk -r ./node_modules/ts-node/register src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "format": "prettier \"**/*.*\" --write",
    "format:check": "prettier \"**/*.*\" --check"
  },
  "author": "Rene Hamburger",
  "license": "MIT",
  "dependencies": {
    "fastq": "^1.8.0",
    "puppeteer": "^4.0.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
    "@types/puppeteer": "^3.0.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  }
}
